    "react-dom": "^18",
    "react-markdown": "^8.0.5",
    "react-router-dom": "^6",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
