{
  "name": "@aws-northstar/ui",
  "version": "0.0.1-development",
  "description": "NorthStar Design System v2",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://aws.github.io/aws-northstar/",
  "keywords": [
    "ui",
    "design-system",
    "front-end",
    "web",
    "react",
    "react-component",
    "aws-northstar"
  ],
  "scripts": {
    "build:readme": "cp ./README.md ./build/README.md",
    "build:copy-files": "cp ../../(LICENSE|NOTICE) ./build/ && cp ./LICENSE-THIRD-PARTY ./build/",
    "build:package-json": "(cat package.json | jq 'del(.devDependencies, .resolutions, .scripts, .\"lint-staged\")') > ./build/package.json",
    "build:package": "tsc --build tsconfig.build.json",
    "build": "yarn build:package && yarn generate:attribution && yarn build:package-json && yarn build:readme && yarn build:copy-files",
    "check:all": "yarn test:ci && yarn build && yarn storybook:build && yarn styleguide:build",
    "generate-attribution": "generate-attribution",
    "generate:attribution": "yarn generate-attribution && mv oss-attribution/attribution.txt LICENSE-THIRD-PARTY",
    "license:check": "yarn license-checker --summary --production --excludePrivatePackages --onlyAllow 'MIT;Apache-2.0;Unlicense;BSD;BSD*;BSD-2-Clause;BSD-3-Clause;ISC;Zlib;WTFPL;'",
    "semantic-release": "semantic-release",
    "storybook:build": "build-storybook -o storybook.out",
    "storybook": "start-storybook -p 6006",
    "test:ci": "yarn test --coverage --silent",
    "test": "jest",
    "test:watch": "jest --runInBand --watch ./src"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-northstar"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@cloudscape-design/jest-preset": "^2.0.6",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash.orderby": "^4.6.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-router-dom": "^5",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-preset-jest": "^29.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.3.1",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "license-checker": "^25.0.1",
    "lodash.orderby": "^4.6.0",
    "merge": "^2.1.1",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^8.0.5",
    "react-router-dom": "^6",
    "react-syntax-highlighter": "^15.5.0",
    "storybook-addon-react-router-v6": "^0.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@cloudscape-design/collection-hooks": "^1.0.20",
    "@cloudscape-design/components": "^3.0.112",
    "@cloudscape-design/design-tokens": "^3.0.7",
    "@cloudscape-design/global-styles": "^1.0.1",
    "@data-driven-forms/react-form-renderer": "^3.19.0",
    "@types/uuid": "^9.0.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "react-qr-code": "^2.0.11",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "use-debounce": "^9.0.3",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-router-dom": "^5",
