    "merge": "^2.1.1",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^8.0.5",
    "react-router-dom": "^6",
    "react-syntax-highlighter": "^15.5.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
