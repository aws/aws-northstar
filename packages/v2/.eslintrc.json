{
    "extends": ["react-app", "plugin:prettier/recommended", "plugin:storybook/recommended"],
    "plugins": ["header"],
    "rules": {
        "header/header": [2, "./header.js"]
    },
    "overrides": [
        {
            "files": ["src/**/*.stories.tsx", "src/__mocks__/*.ts"],
            "rules": {
                "import/no-anonymous-default-export": "off"
            }
        }
    ]
}
